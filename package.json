{
  "name": "aot-migration",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/robertfoobar/angular-aot-issue.git",
  "keywords": [
    "angular2",
    "aot",
    "ahead of time compilation"
  ],
  "scripts": {
    "ngc": "ngc -p ./tsconfig.aot.json --verbose",
    "rimraf": "rimraf",
    "rollup": "rollup -c rollup-config.ts",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "clean": "npm run rimraf -- coverage dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.js --profile --colors --display-error-details --display-cached",
    "prebuild:ci": "npm run clean",
    "build:ci": "webpack --config config/webpack.js --profile --display-error-details --display-cached --bail --env.AOT=true",
    "server": "npm run server:dev",
    "preserver:dev": "npm run build:dev",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.js --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "cross-env NODE_ENV=development npm run server:dev -- --inline --hot",
    "server:prod": "webpack-dev-server --config config/webpack.js --progress --profile --colors --watch --display-error-details --display-cached --content-base src/ --env.AOT=true",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start:prod": "npm run server:prod"
  },
